{
    "manifest_version": 2,
    "name": "CharityClicks",
    "description": "Convert online activities into donations for education projects.",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  